  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:orders": "eslint \"src/features/orders/**/*.{ts,tsx}\" --max-warnings=0",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
